import pygame
pygame.init()
dis = pygame.display.set_mode((500, 500))
pygame.display.update()
pygame.display.set_caption(змейка)
red = [255,0,0]
blue = [0,0,255]
game_over= False
while not game_over:
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            game_over = True
            print(event)
    pygame.draw.rect(dis,blue,[200,150,20,20])
    pygame.draw.rect(dis,blue,[200,125,20,20])
    pygame.draw.rect(dis,blue,[200,100,20,20])
pygame.quit()
quit()
